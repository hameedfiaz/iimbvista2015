<div class="col-md-12" style="border:1px solid black;overflow: scroll;height:100vh;">
	<h3>Event Registrations : </h3><h1><%= @event.title %></h1>
	<div class="well">
		Total User Registrations = <%= @registrations_count %>
	</div>
	<table border=2 width="100%">
		<tr>
			<th width="20%">Team Name</th>
			<th width="30%">Full Name</th>
			<th width="30%">Email</th>
			<th width="20%">Phone</th>
		</tr>
		<% @event.teams.each do |team| %>
			<% team.users.each do |user| %>
				<tr>
					<td width="20%"><%= team.name %></td>
					<td width="25%"><%= user.full_name %></td>
					<td width="25%"><%= user.email %></td>
					<td width="25%"><%= user.phone %></td>
					<td width="15%"><%= user.college.try(:name) %></td>

				</tr>
			<% end %>
		<% end %>
	</table>
	<br/><br/>
	<%= link_to "Back", dashboard_index_path %>
</div>